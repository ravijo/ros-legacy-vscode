# Ignore files inside .vscode-server
.vscode-server/*

# But keep the .gitkeep file to preserve the directory structure
!.vscode-server/.gitkeep

# Ignore everything inside ws/src
ws/src/*

# But keep the .gitkeep file to preserve the directory structure
!ws/src/.gitkeep

# Ignore auto-generated files and folders
.bash_history
.bashrc
.cache/
.config/
.dotnet/
.gitconfig
.gnupg/
.local/
.profile
.ros/
.rviz/
.ssh/

# Ignore temp files
.devcontainer/.onCreateCommandMarker
.devcontainer/.postCreateCommandMarker
.devcontainer/.updateContentCommandMarker
